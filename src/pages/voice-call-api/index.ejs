<script>
    jQuery(document).ready(function () {
		if(jQuery("body").hasClass("page-template-templateprice-new-php")) {
          jQuery(".elementor-tab-mobile-title").attr('style', 'display:none');
		  
		  jQuery('.elementor-tabs').find('.elementor-tabs-wrapper').css('style','block');
        let arr = [];
        jQuery('.elementor-tab-desktop-title').each(function (index, element) {
            arr.push({
                id: element.id,
                text: jQuery('#' + element.id + ' a').text()
            });
        });
        // Mapping of the id and text
        var data123 = jQuery.map(arr, function (obj) {
            obj.id = obj.id || obj.text; // replace pk with your identifier
            return obj;
        });

        jQuery("#customtabdropdown").select2({
            data: data123,
            allowClear: false,
            dropdownPosition: 'below',
            templateResult: formatState,
            templateSelection: formatState,
            placeholder: 'Select Channels/Application',
            escapeMarkup: function (m) {
                return m;
            }
        }).val('').trigger("change");

        function formatState(opt) {
              if (!opt.id) {
                return opt.text.toUpperCase();
            }

            return '<span class="tab-select"><span class="' + opt.text + '"><\/span><\/span><span>' + opt.text.toUpperCase() + '<\/span>';
          }
        
       
         jQuery('#customtabdropdown').on('change', function () {
            jQuery('.select2-selection__rendered').addClass("removeHandImg");
            var element_id = this.value;
            var element_id_arr = element_id.split("-");
            var last_element = element_id_arr[3];  // Get Last Arr Element
           
            if (jQuery(window).width() < 768) {
                jQuery(this).find("option:selected").each(function () {
                    var optionValue = jQuery(this).attr("value");
                    
                    if (optionValue) {
                        jQuery(".elementor-tab-content").not("#elementor-tab-content-" + last_element).hide();
                        jQuery("#elementor-tab-content-" + last_element).show();
                        jQuery('#elementor-tab-content-' + last_element).addClass("elementor-active");
                        jQuery('#elementor-tab-content-' + last_element).show();
                        jQuery('#elementor-tab-content-' + last_element).removeAttr('hidden');
                    } else {
                        jQuery(".elementor-tab-content").hide();
                    }
                });
            }
          });
        }
    });
</script>
