        jQuery('.elementor-tab-desktop-title').each(function (index, element) {
            arr.push({
                id: element.id,
                text: jQuery('#' + element.id + ' a').text()
            });
        });
        // Mapping of the id and text
        var data123 = jQuery.map(arr, function (obj) {
            obj.id = obj.id || obj.text; // replace pk with your identifier
