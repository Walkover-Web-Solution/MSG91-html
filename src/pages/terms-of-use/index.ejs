        function formatState(opt) {
              if (!opt.id) {
                return opt.text.toUpperCase();
            }

            return '<span class="tab-select"><span class="' + opt.text + '"><\/span><\/span><span>' + opt.text.toUpperCase() + '<\/span>';
          }
        
       
         jQuery('#customtabdropdown').on('change', function () {
            jQuery('.select2-selection__rendered').addClass("removeHandImg");
